issn = {8750-7587},
journal = {Journal of applied physiology (Bethesda, Md. : 1985)},
keywords = {Animal,Animals,Computer-Assisted,Confocal,Disease Models,Disease Progression,Imaging,Inbred C57BL,Male,Mice,Microscopy,Pancreatic Elastase,Pulmonary Alveoli,Pulmonary Alveoli: physiopathology,Pulmonary Alveoli: radiography,Pulmonary Emphysema,Pulmonary Emphysema: physiopathology,Pulmonary Emphysema: radiography,Radiographic Image Interpretation,Respiratory Mechanics,Silver Compounds,Silver Compounds: diagnostic use,Staining and Labeling,Staining and Labeling: methods,Three-Dimensional,Time Factors,X-Ray Microtomography},
