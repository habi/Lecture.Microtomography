journal = {The European respiratory journal},
number = {6},
pages = {1633--45},
pmid = {23520313},
title = {{Ex vivo micro-computed tomography analysis of bleomycin-induced lung fibrosis for preclinical drug evaluation.}},
url = {http://erj.ersjournals.com/cgi/doi/10.1183/09031936.00182412{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/23520313},
volume = {42},
year = {2013}
}
