issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
keywords = {ADVECTION,AIR-POLLUTION,Animals,Artificial,Biological,DISEASE,FLOW,Kinetics,Lung: cytology,Lung: physiology,MANAGEMENT,Models,Nonlinear Dynamics,PULMONARY ACINUS,Pulmonary Alveoli,Pulmonary Alveoli: physiology,Rats,Respiration,Sprague-Dawley,Time Factors,UNITED-STATES,aerosol,lung},
month = {jul},
number = {15},
