keywords = {3D thinning,3D vascular tree,ALPT,CT images,GAP,TAP,perforator flaps,plastic surgery,reconstruction},
month = {jan},
pages = {3424--7},
pmid = {17271020},
publisher = {IEEE},
title = {{Automatic 3D vascular tree construction of perforator flaps for plastic surgery planning.}},
