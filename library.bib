issn = {1047-8477},
journal = {Journal of Structural Biology},
keywords = {Animals,Automatic Data Processing,Cellular Structures,Cellular Structures: ultrastructure,Databases,Electron,Electron tomography,Factual,Humans,Imaging,Microscopy,Proteins,Three-Dimensional,Tomography,X-Ray Computed},
number = {1-2},
pages = {145--155},
pmid = {12160711},
title = {{A cell-centered database for electron tomographic data}},
