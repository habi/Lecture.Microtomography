journal = {Journal of structural biology},
keywords = {Animals,Calcium Carbonate,Calcium Carbonate: chemistry,Electron,Invertebrates,Invertebrates: chemistry,Invertebrates: ultrastructure,Microscopy,Scanning,Tomography,X-Ray,X-Ray: methods},
month = {jul},
