@article{Alba-Tercedor2013a,
abstract = {To determine the age if individuals of animal populations is a main goal in ecology, and conservation studies. This is especially crucial in the conservation of endangered species. In the case of reptiles different methodologies have been used (Halliday {\&} Verrell 1988; Yilmaz et al., 2005). Nowadays, skeleto-chronology is the most commonly used method for aging reptiles (Castanet et al.1977; Caetano, 1990; Castanet and Smirina, 1990; Francillon-Viellot et al., 1990). This technique utilizes stained cross- sections of long bones, which allow visualization of lines of arrested growth (LAG). Each LAG corresponds to an annual resting growth period and that the number of LAG represents the age of the animal (Gibbons and McCarthy, 1983; Caetano and Castanet, 1993). Correct interpretation of registered growth marks is not always an easy task, especially in small amphibians or reptiles, when the pattern of LAG in phalanges is less clearly visible, then it may be easier to use bigger bones, such as femur or humerus. Well-defined LAGs are registered in bones when activity is drastically constrained for some period because of harsh environmental conditions(usually during the winter periods). To be able to determine the age of small reptiles, as lizards, assessed by the “skeleto-chronology” technique, herpetologists use to amputate either fingers (in alive individuals), and/or an arm (on voucher specimens) to study the growth lines in cross sections of the bones. The process to prepare samples is tedious and high time consuming. It needs to demineralize bones, to cut them in a cryo-microtome to get cross-section slices of bones; later they must pass process with different chemicals to be able to fix and stain the tissues before to mount them in slides, before to study the samples under the light microscope. Moreover when dealing with endangered species, the injury of any amputation, even a finger, represents a serious problem from a conservational, and ethical, point of view. Thus with the aim to find a faster and less injuring methodology to find growth patterns on reptiles, we decided to investigate the possibilities of the microtomography.},
author = {Alba-Tercedor, Javier and Comas, Mar and Reguera, Senda and Zamora-Camacho, Francisco Javier and Pleguezuelos, Juan Manuel},
file = {::},
journal = {Bruker Micro-CT Users Meeting 2013},
pages = {219--223},
title = {{How micro-CT can help zoologists to determine the age of reptiles}},
year = {2013}
}
