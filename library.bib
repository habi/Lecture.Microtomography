file = {:home/habi/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Knust et al. - 2009 - Stereological estimates of alveolar number and size and capillary length and surface area in mice lungs.pdf:pdf},
issn = {1932-8494},
journal = {Anatomical record (Hoboken, N.J.: 2007)},
keywords = {Animals,Capillaries,Capillaries: anatomy {\&} histology,Capillaries: cytology,Cell Count,Cell Count: methods,Cell Count: statistics {\&} numerical data,Cell Size,Female,Inbred C57BL,Lung,Lung: anatomy {\&} histology,Lung: blood supply,Lung: cytology,Mice,Pulmonary Alveoli,Pulmonary Alveoli: anatomy {\&} histology,Pulmonary Alveoli: blood supply,Pulmonary Alveoli: cytology},
month = {jan},
