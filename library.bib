issn = {2167-8359},
journal = {PeerJ},
pages = {e355},
pmid = {24860694},
title = {{An interactive three dimensional approach to anatomical description—the jaw musculature of the Australian laughing kookaburra ( {\textless}i{\textgreater}Dacelo novaeguineae{\textless}/i{\textgreater} )}},
url = {https://peerj.com/articles/355},
volume = {2},
year = {2014}
}
