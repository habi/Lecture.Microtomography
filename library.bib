pages = {10178},
pmid = {25985192},
publisher = {Nature Publishing Group},
title = {{Ultra High-Resolution In vivo Computed Tomography Imaging of Mouse Cerebrovasculature Using a Long Circulating Blood Pool Contrast Agent}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4650815{\&}tool=pmcentrez{\&}rendertype=abstract http://www.nature.com/articles/srep10178},
