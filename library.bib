issn = {1542-4863},
journal = {CA: a cancer journal for clinicians},
keywords = {Female,Health Status Disparities,Humans,Incidence,Male,Mortality,Neoplasms,Neoplasms: epidemiology,Neoplasms: ethnology,Neoplasms: mortality,Socioeconomic Factors,United States,United States: epidemiology},
number = {4},
pages = {212--36},
pmid = {21685461},
title = {{Cancer statistics, 2011: the impact of eliminating socioeconomic and racial disparities on premature cancer deaths.}},
