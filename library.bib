pages = {012040},
title = {{Multimodal imaging for the detection of sub-micron particles in the gas-exchange region of the mammalian lung}},
url = {http://doi.org/10.1088/1742-6596/186/1/012040 http://stacks.iop.org/1742-6596/186/i=1/a=012040?key=crossref.084d800c2b7907e5a291384874e386f5},
