journal = {Theranostics},
keywords = {Light sheet fluorescence microscopy,Micro-CT,Ultra microscopy},
number = {8},
pages = {2117--2133},
pmid = {29721067},
title = {{3D Imaging and Quantitative Analysis of Vascular Networks: A Comparison of Ultramicroscopy and Micro-Computed Tomography}},
url = {http://www.thno.org/v08p2117.htm},
