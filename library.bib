issn = {1525-142X},
journal = {Evolution {\&} development},
keywords = {Animals,Embryonic Development,Embryonic Development: physiology,Environment,Fossils,Invertebrates,Invertebrates: anatomy {\&} histology,Invertebrates: embryology,Paleontology,Paleontology: methods,Seawater,Seawater: chemistry,Species Specificity},
number = {3},
pages = {339--49},
pmid = {18460095},
publisher = {Wiley Online Library},
title = {{Deciphering the fossil record of early bilaterian embryonic development in light of experimental taphonomy.}},
url = {http://doi.org/10.1111/j.1525-142X.2008.00242.x},
volume = {10},
year = {2008}
}
