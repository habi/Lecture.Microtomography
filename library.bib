journal = {The European respiratory journal : official journal of the European Society for Clinical Respiratory Physiology},
keywords = {Animals,Body Weights and Measures,Lung,Lung: radiography,Swine,Tomography,X-Ray Computed,X-Ray Computed: methods},
month = {oct},
