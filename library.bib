file = {:home/habi/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/M{\"{u}}ller et al. - 1998 - Morphometric analysis of human bone biopsies a quantitative structural comparison of histological sections and m.pdf:pdf},
issn = {8756-3282},
journal = {Bone},
keywords = {80 and over,Adult,Aged,Biopsy,Computer-Assisted,Female,Histological Techniques,Humans,Ilium,Ilium: pathology,Ilium: radiography,Image Processing,Male,Middle Aged,Miniaturization,Osteoporosis,Osteoporosis: pathology,Tomography,X-Ray Computed,X-Ray Computed: instrumentation,X-Ray Computed: methods},
