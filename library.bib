@article{Weibel1992,
abstract = {We explore the question of whether and to what extent the large variation in energy requirements observed among mammals is related to variations in the design of the respiratory system, from the lung to the mitochondria in muscle cells. Resting metabolic rate is determined by body size (allometric variation). Maximal rates of O 2 consumption (V̇ O2 max) also vary in a regular manner with body size, but adaptive variation allows some species to achieve much higher values than others of the same body size. We, therefore, consider adaptive variation as modulation of structures and functions above those determined by allometric variation. A model is presented that separates functional and design parameters at four steps of the respiratory cascade: the pulmonary gas exchanger, heart and blood, microvasculature, and mitochondria. The variations observed in these parameters are analyzed with respect to those in energy demand and are discussed in relation to the hypothesis of symmorphosis. We conclude that the design of the internal steps of the respiratory system (mitochondria, capillaries, blood, and heart) is matched to functional demand, whereas the lung maintains a variable excess of morphometric diffusing capacity which may be related to the facts that the lung has limited malleability and that it forms the interface with the environment. {\textcopyright} 1992.},
author = {Weibel, Ewald R. and {Richard Taylor}, C. and Hoppeler, Hans},
doi = {10.1016/0034-5687(92)90015-O},
issn = {00345687},
journal = {Respiration Physiology},
keywords = {Diffusing capacity,Maximal O 2 consumption,O 2 exchange,O 2 transport system,Oxygen transport,Skeletal muscle,design,function,lung},
month = {mar},
number = {3},
pages = {325--348},
publisher = {Elsevier},
title = {{Variations in function and design: Testing symmorphosis in the respiratory system}},
url = {https://www.sciencedirect.com/science/article/pii/003456879290015O?via{\%}3Dihub https://linkinghub.elsevier.com/retrieve/pii/003456879290015O},
volume = {87},
year = {1992}
}
