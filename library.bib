@article{Rekdal2014,
abstract = {Many of the messages presented in respectable scientific publications are, in fact, based on various forms of rumors. Some of these rumors appear so frequently, and in such complex, colorful, and entertaining ways that we can think of them as academic urban legends. The explanation for this phenomenon is usually that authors have lazily, sloppily, or fraudulently employed sources, and peer reviewers and editors have not discovered these weaknesses in the manuscripts during evaluation. To illustrate this phenomenon, I draw upon a remarkable case in which a decimal point error appears to have misled millions into believing that spinach is a good nutritional source of iron. Through this example, I demonstrate how an academic urban legend can be conceived and born, and can continue to grow and reproduce within academia and beyond.},
author = {Rekdal, O. B.},
doi = {10.1177/0306312714535679},
issn = {0306-3127},
journal = {Social Studies of Science},
month = {jun},
number = {4},
pages = {638--654},
title = {{Academic urban legends}},
url = {http://sss.sagepub.com/content/44/4/638.long},
volume = {44},
year = {2014}
}
@article{Rothmaier2008,
abstract = {Biomedical sensors, integrated into textiles would enable monitoring of many vitally important physiological parameters during our daily life. In this paper we demonstrate the design and performance of a textile based pulse oximeter, operating on the forefinger tip in transmission mode. The sensors consisted of plastic optical fibers integrated into common fabrics. To emit light to the human tissue and to collect transmitted light the fibers were either integrated into a textile substrate by embroidery (producing microbends with a nominal diameter of 0.5 to 2 mm) or the fibers inside woven patterns have been altered mechanically after fabric production. In our experiments we used a two-wavelength approach (690 and 830 nm) for pulse wave acquisition and arterial oxygen saturation calculation. We have fabricated different specimens to study signal yield and quality, and a cotton glove, equipped with textile based light emitter and detector, has been used to examine movement artifacts. Our results show that textile-based oximetry is feasible with sufficient data quality and its potential as a wearable health monitoring device is promising.},
