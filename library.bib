keywords = {Computer Simulation,Computer-Assisted,Cryoelectron Microscopy,Fourier Analysis,Image Processing, Computer-Assisted,Imaging, Three-Dimensional,Models, Statistical,Models, Theoretical,Statistical,Theoretical,Three-Dimensional},
month = {sep},
