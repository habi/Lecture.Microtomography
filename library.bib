isbn = {1522-1601},
issn = {8750-7587},
journal = {Journal of Applied Physiology},
keywords = {fixed lung,high-resolution imaging,lung morphometry,mouse,quantitative CT},
month = {mar},
number = {6},
pages = {716--724},
pmid = {23264542},
