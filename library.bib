pages = {e59743},
publisher = {Public Library of Science},
title = {{Cunnilingus apparently increases duration of copulation in the Indian flying fox, Pteropus giganteus.}},
url = {http://dx.plos.org/10.1371/journal.pone.0059743},
volume = {8},
year = {2013}
}
