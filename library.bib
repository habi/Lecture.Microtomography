editor = {Mattei, Fabrizio},
file = {:home/habi/P/Mendeley/Potez et al/Potez et al. - 2018 - Characterization of a B16-F10 melanoma model locally implanted into the ear pinnae of C57BL6 mice.pdf:pdf},
isbn = {1111111111},
issn = {1932-6203},
journal = {PLOS ONE},
month = {nov},
number = {11},
pages = {e0206693},
pmid = {30395629},
title = {{Characterization of a B16-F10 melanoma model locally implanted into the ear pinnae of C57BL/6 mice}},
url = {https://doi.org/10.1371/journal.pone.0206693},
volume = {13},
year = {2018}
}
