issn = {10991492},
journal = {NMR in Biomedicine},
keywords = {Anti-angiogenic,Blood volume,Glioma,MRI,Multiparametric,Oxygenation,Permeability,Vessel size},
month = {sep},
number = {9},
pages = {1163--1173},
pmid = {26224287},
title = {{Multiparametric MRI as an early biomarker of individual therapy effects during concomitant treatment of brain tumours}},
