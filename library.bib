issn = {0021-8782},
journal = {Journal of Anatomy},
keywords = {Adult,Anatomic,Bronchography,Humans,Imaging,Lung,Lung: radiography,Male,Models,Three-Dimensional,Tomography,X-Ray Computed},
month = {feb},
number = {2},
pages = {123},
pmid = {11895110},
publisher = {Blackwell Publishing},
title = {{Study of the three-dimensional geometry of the central conducting airways in man using computed tomographic (CT) images}},
