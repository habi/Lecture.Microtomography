journal = {Proc Am Thorac Soc},
keywords = {Animals,Imaging,Lung,Lung: radiography,Mice,Pulmonary Artery,Pulmonary Artery: radiography,Pulmonary Veins,Pulmonary Veins: radiography,Rats,Three-Dimensional,Tomography,Tomography Scanners,X-Ray Computed,X-Ray Computed: instrumentation,X-Ray Computed: methods},
