journal = {The anatomical record. Part A, Discoveries in molecular, cellular, and evolutionary biology},
keywords = {Age Factors,Animals,Cell Count,Cell Count: instrumentation,Cell Count: methods,Cell Fractionation,Cell Fractionation: instrumentation,Cell Fractionation: methods,Cell Size,Cell Size: physiology,Female,Macaca mulatta,Male,Pulmonary Alveoli,Pulmonary Alveoli: cytology,Pulmonary Alveoli: physiology,Rats,Statistics as Topic,Wistar},
