journal = {Materials},
keywords = {Ice-templating,Image analysis,Nanofibrillated cellulose,Polysaccharide,Synchrotron microtomography},
month = {jul},
number = {8},
pages = {871},
publisher = {Multidisciplinary Digital Publishing Institute},
title = {{Synchrotron Microtomography Reveals the Fine Three-Dimensional Porosity of Composite Polysaccharide Aerogels}},
