keywords = {Adult,Anatomic,Bronchi,Bronchi: anatomy {\&} histology,Electron,Humans,Lung,Lung: anatomy {\&} histology,Microscopy,Models,Pulmonary Alveoli,Pulmonary Alveoli: anatomy {\&} histology,Scanning,Silicone Elastomers},
month = {apr},
