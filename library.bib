keywords = {Biopsy,Elastic Tissue,Elastic Tissue: pathology,Humans,Imaging,Interstitial,Interstitial: etiology,Interstitial: pathology,Lung Diseases,Three-Dimensional},
month = {jul},
number = {1},
pages = {47--52},
pmid = {12111200},
publisher = {Springer},
title = {{Three-dimensional analysis of alveolar structure in usual interstitial pneumonia.}},
url = {http://dx.doi.org/10.1007/s00428-001-0567-8},
volume = {441},
year = {2002}
}
