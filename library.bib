keywords = {Animals,Blood Pressure,Dogs,Electron,Lung,Lung Volume Measurements,Lung: physiology,Microscopy,Pressure,Pulmonary Alveoli,Pulmonary Alveoli: ultrastructure,Pulmonary Surfactants,Pulmonary Surfactants: physiology,Pulmonary Ventilation,Regression Analysis,Scanning},
